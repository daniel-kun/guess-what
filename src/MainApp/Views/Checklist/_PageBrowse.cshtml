
@model IEnumerable<Io.GuessWhat.MainApp.Models.ChecklistBrowseItem>

<!-- Fill out checklist -->
<div class="container-fluid main-section" id="browse-page">
    <div class="row">
        <div class="panel panel-default" style="margin-left: 5%; margin-right: 5%;">
            <div class="panel-heading" style="position:relative">
                <div class="panel-heading">
                    Browse some favorite checklists
                </div>
                <a class="btn btn-default" style="color:#000;position:absolute;top:10px;right:11px;" href="/c/new">Create your own checklist</a>
            </div>
            <div class="panel-body">
                <table class="table" style="color: darkgray">
                    <tbody>
                        @foreach (var item in Model)
                        {
                            await Html.RenderPartialAsync("_PageBrowseRow", item);
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- Fill out checklist -->
